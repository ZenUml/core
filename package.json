{
  "name": "vue-sequence",
  "version": "0.9.6",
  "private": false,
  "scripts": {
    "build": "vue-cli-service build --target lib --modern --name vue-sequence ./src/index.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "babel": "babel --presets es2015 dist/vue-sequence.umd.js -o dist/vue-sequence.umd.es5.js",
    "build-crx": "vue-cli-service build --mode production --target lib --name vue-sequence ./src/index.js",
    "start": "vue-cli-service serve"
  },
  "main": "./dist/vue-sequence.umd.min.js",
  "dependencies": {
    "@vue/cli": "^3.0.0-rc.5",
    "sequence-parser": "^0.8.6",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@vue/cli-plugin-babel": "^3.0.0-rc.2",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.2",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-service": "^3.0.0-rc.2",
    "@vue/test-utils": "^1.0.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "svg-url-loader": "^5.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
