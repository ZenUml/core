{
  "name": "vue-sequence",
  "version": "0.7.5",
  "main": "./dist/vue-sequence.umd.min.js",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "babel": "babel --presets es2015 dist/vue-sequence.umd.js -o dist/vue-sequence.umd.es5.js",
    "build": "vue-cli-service build --target lib --modern --name vue-sequence ./src/index.js",
    "build-crx": "vue-cli-service build --mode production --target lib --name vue-sequence ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/cli": "^3.0.0-rc.5",
    "sequence-parser": "^0.6.1",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.2",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.2",
    "@vue/cli-service": "^3.0.0-rc.2",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
