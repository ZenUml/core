{
  "name": "vue-sequence",
  "version": "0.9.9",
  "private": false,
  "scripts": {
    "build": "vue-cli-service build --target lib --modern --name vue-sequence ./src/index.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "antlr:clear": "rm -rf src/generated-parser",
    "antlr:lexer": "cd src/g4 && antlr4 -o ../generated-parser/ -Dlanguage=JavaScript ./sequenceLexer.g4",
    "antlr:parser": "cd src/g4 && antlr4 -o ../generated-parser/ -Dlanguage=JavaScript ./sequenceParser.g4",
    "antlr": "yarn antlr:clear && yarn antlr:lexer && yarn antlr:parser",
    "babel": "babel --presets es2015 dist/vue-sequence.umd.js -o dist/vue-sequence.umd.es5.js",
    "build-crx": "vue-cli-service build --mode production --target lib --name vue-sequence ./src/index.js",
    "start": "vue-cli-service serve"
  },
  "main": "./dist/vue-sequence.umd.min.js",
  "dependencies": {
    "antlr4": "^4.8.0",
    "vue": "^2.5.16",
    "vuex": "^3.0.1",
    "gauge-ts": "0.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-arrow-functions": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@vue/cli": "^3.0.0-rc.5",
    "@vue/cli-plugin-babel": "^3.0.0-rc.2",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.2",
    "@vue/cli-plugin-unit-jest": "~4.3.0",
    "@vue/cli-service": "^3.0.0-rc.2",
    "@vue/test-utils": "^1.0.3",
    "core-js": "2.6.5",
    "svg-url-loader": "^5.0.0",
    "taiko": "^1.0.11",
    "terser-webpack-plugin": "^2.3.5",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "@types/node": "latest"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
